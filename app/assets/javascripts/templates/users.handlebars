<h1>Users...</h1>
<div class="span3">
  <table class='table table-striped'>
    <tr>
      <th>ID</th>
      <th>Name</th>
    </tr>
    {{#each controller}}
  		<tr>
  			<td>{{id}}</td>
  			<td>{{#link-to 'users.show' this}}{{fullName}}{{/link-to}}</td>
			</tr>
		{{/each}}
  </table>
  {{#link-to 'users.new' class='btn btn-primary'}}Add User{{/link-to}}
</div>
<div class="span8">
	{{outlet}}
</div>

